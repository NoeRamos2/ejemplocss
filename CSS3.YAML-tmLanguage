      '2': {name: punctuation.definition.keyword.css}

  selector-pseudo-class:
    name: entity.other.attribute-name.pseudo-class.css
    match: (:)(?:visited|valid|user-error|target|scope|root|required|read-write|read-only|placeholder-shown|past|out-of-range|optional|only-of-type|only-child|link|last-of-type|last-child|invalid|indeterminate|in-range|hover|host|future|focus|first-of-type|first-child|enabled|empty|drop|disabled|default|current|checked|blank|any-link|active)\b
