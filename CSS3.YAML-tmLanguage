  # The CSS Box Alignment Module Level 3 spec supersedes the values listed in the Flexbox spec.
  value-align-content-justify-content:
    begin: (?<=align-content:|justify-content:)
    patterns:
    - include: '#comment-block'
    - include: '#type-important'
    - include: '#type-all'
    - include: '#type-function-attr'
    - include: '#type-function-toggle'
    - include: '#type-function-var'
    - name: support.constant.property-value.css
      match: \b(?:unsafe|stretch|start|space-evenly|space-between|space-around|safe|right|normal|left|last-baseline|flex-start|flex-end|end|center|baseline)\b
    # Ending before a closing paren makes these rules usable inside an @supports
    # condition.
    end: (?=[;)])

