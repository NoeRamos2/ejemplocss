  media-feature-name:
    match: (?<=[><=(])\s*(width|update|scripting|scan|resolution|pointer|overflow-inline|overflow-block|orientation|monochrome|min-width|min-resolution|min-monochrome|min-height|min-device-width|min-device-radius|min-device-height|min-device-aspect-ratio|min-color-index|min-color|min-aspect-ratio|max-width|max-resolution|max-monochrome|max-height|max-device-width|max-device-radius|max-device-height|max-device-aspect-ratio|max-color-index|max-color|max-aspect-ratio|light-level|inverted-colors|hover|height|grid|device-width|device-radius|device-height|device-aspect-ratio|color-index|color-gamut|color|aspect-ratio|any-pointer|any-hover)\s*(?=[><=):])
    captures:
      '1': {name: support.constant.property-value.css}

  media-feature-property-color-gamut:
