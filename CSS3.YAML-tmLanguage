    end: (?=:)

  # This rule supports custom elements, like <polymer-widget>.
  property-name-custom:
    name: support.type.property-name.custom.css
    match: --[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*

  property-name-illegal:
    name: invalid.illegal.css
    match: \b(?:nav-index|ime-mode|icon|font-smooth|clip|behavior|accelerator)\b(?!\-)

  property-name-illegal-moz:
    name: invalid.illegal.css
    match: -moz-(?:user-modify|user-input|user-focus|box-pack|box-orient|box-ordinal-group|box-lines|box-flex-group|box-flex|box-direction|box-align|binding)\b

  property-name-illegal-ms:
    name: invalid.illegal.css
    match: -ms-(?:text-kashida-space|progress-appearance|layout-flow|interpolation-mode|ime-mode|block-progression|behavior|accelerator)\b

  property-name-illegal-webkit:
    name: invalid.illegal.css
    match: -webkit-(?:user-modify|user-input|user-focus|box-reflect|box-pack|box-orient|box-ordinal-group|box-lines|box-flex-group|box-flex|box-direction|box-align)\b

  property-name-deprecated:
    name: invalid.deprecated.css
    match: (?<!\-)\b(?:page-break-inside|page-break-before|page-break-after|kerning|glyph-orientation-vertical|glyph-orientation-horizontal|clip)\b((?!\-))

