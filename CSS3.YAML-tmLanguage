  # (aspect-ratio: 16/9)
  media-feature-property-ratio:
    match: (?<=min-device-aspect-ratio:|min-aspect-ratio:|max-device-aspect-ratio:|max-aspect-ratio:|device-aspect-ratio:|aspect-ratio:)\s*(\+?(?:[1-9]\d+|[1-9]))\s*\/\s*(\+?(?:[1-9]\d+|[1-9]))\s*(?=\))
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: constant.numeric.css}

  # (min-resolution: 300dpi)
  media-feature-property-resolution:
    match: (?<=max-resolution:|min-resolution:|resolution:)\s*(\+?(?:[1-9]\d+|[1-9]))(dppx|dpi|dpcm)\s*(?=\))
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}

  media-feature-property-scan:
    match: (?<=scan:)\s*(progressive|interlace)\s*(?=\))
    captures:
      '1': {name: support.constant.property-value.css}

  media-feature-property-scripting:
