  transition-value-height:
    begin: (?<=min-height|max-height|height)\s+
    patterns:
    - include: '#comment-block'
    - include: '#type-important'
    - include: '#type-all'
    - include: '#type-function-attr'
    - include: '#type-function-toggle'
    - include: '#type-function-var'
    # none is technically a value of max-height, not min-height, but it's not
    # worth writing a separate rule. Also, the extend-to-zoom attribute is only
    # for when width: is used as a @viewport descriptor, but is included here
    # for completeness. Do not use extend-to-zoom outside of an @viewport rule.
    - name: support.constant.property-value.css
      match: \b(?:none|min-content|max-content|fit-content|extend-to-zoom|content-box|complex|border-box|available|auto)\b
