    captures:
      '1': {name: support.constant.property-value.css}

  # (width: 600px)
  media-feature-property-length:
    begin: (?<=width:|min-width:|min-height:|min-device-width:|min-device-height:|max-width:|max-height:|max-device-width:|max-device-height:|height:|device-width:|device-height:)
