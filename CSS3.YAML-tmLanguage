    - name: support.constant.property-value.css
      match: \b(?:thin|thick|medium)\b
    - include: '#type-line-style'
    - include: '#type-function-hwb'
    - include: '#type-function-device-cmyk'
    - include: '#type-function-color'
    - include: '#type-function-gray'
    - include: '#type-color-named-deprecated'
    - include: '#type-color-named'
