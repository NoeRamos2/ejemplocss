  type-important:
    name: keyword.other.important.css
    match: '!important\b'

  type-integer:
    name: constant.numeric.css
    match: '[+-]?\d+'

  type-integer-non-negative:
    name: constant.numeric.css
    match: '\+?\d+'

  type-integer-positive:
    name: constant.numeric.css
    match: \+?(?:[1-9]\d+|[1-9])

  type-length:
    match: (?:([+-]?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(r?em|ex|ch|vw|vh|vmin|vmax|cm|mm|q|in|pc|pt|px)\b|\b(0)\b)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-length-absolute:
    match: (?:([+-]?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(cm|mm|q|in|pc|pt|px)\b|\b(0)\b)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-length-non-negative:
    match: (?:(\+?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(r?em|ex|ch|vw|vh|vmin|vmax|cm|mm|q|in|pc|pt|px)\b|\b(0)\b)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-length-positive:
    match: (\+?(?:[0-9]+\.[0-9]*[1-9][0-9]*|\.[0-9]*[1-9][0-9]*|[1-9][0-9]*)(?:[eE][+-]?[0-9]+)?)(r?em|ex|ch|vw|vh|vmin|vmax|cm|mm|q|in|pc|pt|px)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-line-name:
    begin: \[
    beginCaptures:
      '0': {name: punctuation.section.line-name.css}
    patterns:
    - include: '#comment-block'
    - include: '#identifier'
    end: \]
    endCaptures:
      '0': {name: punctuation.section.line-name.css}

  type-line-style:
    name: support.constant.property-value.css
    match: \b(?:solid|ridge|outset|none|inset|hidden|groove|double|dotted|dashed)\b

  type-number:
    name: constant.numeric.css
    match: '[+-]?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?'

  type-number-non-negative:
    name: constant.numeric.css
    match: \+?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?

  type-number-zero-to-one:
    name: constant.numeric.css
    match: '[01]?\.0+|0?\.\d*[1-9]\d*|[01]'

  type-percentage:
    match: (?:([+-]?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(%)|\b(0)\b)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-percentage-hundred-max:
    # this rule enforces the constraint that the percentage must be 0-100%.
    # note: scientific notation is not supported.
    match: (?:((?:100|\d\d|\d)(?:\.\d+)?|(?:\.\d+))(%)|\b(0)\b)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-percentage-non-negative:
    match: (?:(\+?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(%)|\b(0)\b)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}
      '3': {name: constant.numeric.css}

  type-percentage-positive:
    match: (\+?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(%)
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}

  type-ratio:
    match: (\+?(?:[1-9]\d+|[1-9]))\s*\/\s*(\+?(?:[1-9]\d+|[1-9]))
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: constant.numeric.css}

  type-repeat-style:
    name: support.constant.property-value.css
    match: \b(?:space|round|repeat-y|repeat-x|repeat|no-repeat)\b

  type-resolution:
    match: (\+?(?:[0-9]+\.[0-9]+|\.[0-9]+|[0-9]+)(?:[eE][+-]?[0-9]+)?)(dppx|dpcm|dpi)\b
    captures:
      '1': {name: constant.numeric.css}
      '2': {name: keyword.other.unit.css}

  type-string-double:
