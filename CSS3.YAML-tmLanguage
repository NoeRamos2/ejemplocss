  property-name-supports-condition:
    name: meta.supports-condition-property-name.css
    begin: \(
    beginCaptures:
      '0': {name: punctuation.section.supports-condition.css}
    patterns:
    - include: '#comment-block'
    - include: '#property-name-illegal-webkit'
    - include: '#property-name-illegal-moz'
    - include: '#property-name-illegal-ms'
    - include: '#property-name-illegal'
    - include: '#property-name-deprecated'
    - include: '#property-name-legal'
    - include: '#property-name-custom'
    end: (?=:)

  selector:
    # Selectors can start with a lowercase letter, digit (100%), hyphen (-moz-),
    # colon (:not()), period (.banner), hash (#intro), bracket ([value="1"]),
    # or the wildcard (*). The W3C is currently deciding whether the parent
    # selector will be determined by prepending or appending an exclamation mark
    # (!ol li vs ol! li). The exclamation mark may have to be added later.

    name: meta.selector.css
    begin: (?=[@\d:.#*\p{L}\[-])
    patterns:
    - include: '#comment-block'
    - include: '#selector-pseudo-element-webkit'
    - include: '#selector-pseudo-element-moz'
    - include: '#selector-pseudo-element-function-blank-slot'
    - include: '#selector-pseudo-element'
    - include: '#selector-element-deprecated'
    - include: '#selector-pseudo-class-deprecated'
    - include: '#selector-pseudo-class-function-dir'
    - include: '#selector-pseudo-class-function-lang'
    - include: '#selector-pseudo-class-function-locale-dir'
    - include: '#selector-pseudo-class-function-system-metric'
    - include: '#selector-pseudo-class-function-tree'
    - include: '#selector-pseudo-class-moz'
    - include: '#selector-pseudo-class-ms'
    - include: '#selector-pseudo-class-webkit'
    - include: '#selector-pseudo-class-function-drop'
    - include: '#selector-pseudo-class-function-an-plus-b'
    - include: '#selector-pseudo-class-function-selector-list'
    - include: '#selector-pseudo-class-function-nth'
    - include: '#selector-pseudo-class-function-selector-list-an-plus-b'
    - include: '#selector-pseudo-class'
    - include: '#selector-element'
    - include: '#selector-id'
    - include: '#selector-class'
    - include: '#selector-keyframes'
    - include: '#selector-attribute'
    - include: '#selector-combinator'
    - include: '#selector-page-margin'
    # The at-keyword selector matches keyframe animation types like @swash
    - include: '#selector-at-keyword'
    # TODO: enable this. right now it's matching **everything** because of the <ident>
    # - include: '#selector-custom-element'
    end: (?=[{;])

  selector-attribute:
    begin: \[
    beginCaptures:
      '0': {name: punctuation.section.attribute.css}
    patterns:
    - include: '#comment-block'
    - name: keyword.operator.attribute.css
      match: '[~^$*|]?='
    - include: '#selector-attribute-name'
    - include: '#type-string-double'
    - include: '#type-string-single'
    - name: string.unquoted.attribute-value.css
      match: -?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*
    end: \]
    endCaptures:
      '0': {name: punctuation.section.attribute.css}

  selector-attribute-name:
    match: (?<=\[)\s*(?:(?:(-?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*)|(\*))?\s*(\|))?\s*(?:(-?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*)|(\*))\s*(?=[~^$*=|\]])
    captures:
      '1': {name: entity.other.attribute-name.namespace.css}
      '2': {name: entity.other.attribute-name.namespace.wildcard.css}
      '3': {name: punctuation.separator.namespace.css}
      '4': {name: entity.other.attribute-name.css}
      '5': {name: entity.other.attribute-name.wildcard.css}

  selector-at-keyword:
    name: keyword.control.at-keyword.css
    match: (@)-?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*
    captures:
      '1': {name: punctuation.definition.keyword.css}

  selector-class:
    name: entity.other.attribute-name.class.css
    match: (\.)-?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*
    captures:
      '1': {name: punctuation.definition.entity.css}

  selector-combinator:
    name: keyword.operator.selector-combinator.css
    match: \|\||[+~>]|/-?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*/

  # TODO: enable this rule
  # This rule matches custom elements, e.g. <polymer-widget>.
  selector-custom-element:
    # This rule matches non-uppercase, non-titlecase letter, optionally followed
    # by letters, digits, combining marks, periods, underscores, or hyphens.
    name: entity.name.tag.custom.css
    match: '[-\p{Ll}\p{Lm}\p{Lo}][-\p{Ll}\p{Lm}\p{Lo}\d._\p{M}]*'

  selector-element:
    match: ((?:-?[a-zA-Z_\\\p{^ASCII}][-a-zA-Z\d_\\\p{^ASCII}]*|\*)\s*(\|))?\s*(?:(?<![:-])\b(wbr|view|video|var|use|unknown|ul|u|tspan|track|tr|title|time|thead|th|tfoot|textPath|textarea|text|template|td|tbody|table|symbol|switch|svg|sup|summary|sub|style|strong|stop|span|source|solidcolor|small|set|select|section|script|samp|s|ruby|rt|rp|rect|radialGradient|q|progress|pre|polyline|polygon|pattern|path|param|p|output|option|optgroup|ol|object|noscript|nav|mpath|meter|metadata|meta|meshrow|meshpatch|mesh|menuitem|menu|mask|marker|mark|map|main|link|linearGradient|line|li|legend|label|keygen|kbd|ins|input|img|image|iframe|i|html|hr|header|head|hatchpath|hatch|h6|h5|h4|h3|h2|h1|g|form|foreignObject|footer|filter|figure|figcaption|fieldset|feTurbulence|feTile|feSpotLight|feSpecularLighting|fePointLight|feOffset|feMorphology|feMergeNode|feMerge|feImage|feGaussianBlur|feFuncR|feFuncG|feFuncB|feFuncA|feFlood|feDropShadow|feDistantLight|feDisplacementMap|feDiffuseLighting|feConvolveMatrix|feComposite|feComponentTransfer|feColorMatrix|feBlend|embed|em|ellipse|dt|dl|div|discard|dialog|dfn|details|desc|del|defs|dd|datalist|data|cursor|colgroup|col|code|clipPath|cite|circle|caption|canvas|button|br|body|blockquote|bdo|bdi|base|b|audio|aside|article|area|animateTransform|animateMotion|animate|address|abbr|a)\b|(\*))(?!-)
    captures:
      '1': {name: string.unquoted.namespace.css}
      '2': {name: punctuation.separator.namespace.css}
      '3': {name: entity.name.tag.css}
      '4': {name: entity.name.tag.wildcard.css}

  selector-element-deprecated:
    name: invalid.deprecated.css
    match: (?<![-:\[])\b(?:xmp|tt|strike|spacer|rtc|rb|plaintext|noframes|noembed|nobr|nextid|multicol|marquee|listing|isindex|hgroup|frameset|frame|font|dir|center|blink|big|bgsound|basefont|applet|acronym)\b(?!-)

