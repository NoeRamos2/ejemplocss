    end: (?=[;)])

  value-width:
    begin: (?<=width:|max-width:|min-width:)
    patterns:
    - include: '#comment-block'
    - include: '#type-important'
    - include: '#type-all'
    - include: '#type-function-attr'
    - include: '#type-function-toggle'
    - include: '#type-function-var'
    # The extend-to-zoom attribute is only for when width: is used as a @viewport
    # descriptor, but is included here for completeness. Do not use extend-to-zoom
    # outside of an @viewport rule.
    - name: support.constant.property-value.css
      match: \b(?:none|min-content|max-content|fit-content|extend-to-zoom|content-box|border-box|available|auto)\b
    - include: '#type-function-calc'
    - include: '#type-percentage-non-negative'
    - include: '#type-length-non-negative'
    end: (?=[;)])

