  # NOTE: break-before, break-after, and break-inside are described in 3 specs: CSS Regions Module
  # Level 1, CSS Multi-column Layout Module, and CSS Fragmentation Module Level 3. The specs are
  # similar but not identical. This rule is a superset that matches all three.
  value-break-after-break-before:
    begin: (?<=break-after:|break-before:)
    patterns:
    - include: '#comment-block'
    - include: '#type-important'
    - include: '#type-all'
    - include: '#type-function-attr'
    - include: '#type-function-toggle'
    - include: '#type-function-var'
    - name: support.constant.property-value.css
      match: \b(?:verso|right|region|recto|page|left|column|avoid-region|avoid-page|avoid-column|avoid|auto|always)\b
    end: (?=[;)])

  # NOTE: break-before, break-after, and break-inside are described in 3 specs: CSS Regions Module
  # Level 1, CSS Multi-column Layout Module, and CSS Fragmentation Module Level 3. The specs are
  # similar but not identical. This rule is a superset that matches all three.
  value-break-inside:
    begin: (?<=break-inside:)
    patterns:
    - include: '#comment-block'
    - include: '#type-important'
    - include: '#type-all'
    - include: '#type-function-attr'
    - include: '#type-function-toggle'
    - include: '#type-function-var'
    - name: support.constant.property-value.css
      match: \b(?:avoid-region|avoid-page|avoid-column|avoid|auto)\b
    end: (?=[;)])

