  type-function-rgb:
    # this rule has been modified to match the rgb() color adjuster as well as
    # the rgb() color function. matching both with one rule resolves the
    # collision, but makes the rule less specific (less likely to catch
    # mistakes)
    begin: \b(rgb)(\()
    beginCaptures:
      '1': {name: support.function.rgb.css}
      '2': {name: punctuation.section.function.css}
    contentName: meta.function.rgb.css
    patterns:
    - include: '#comment-block'
    - include: '#type-function-calc'
    - name: keyword.operator.arithmetic.css
      match: (?<=[\(\s])[\+\-\*]
    - include: '#type-percentage'
    - include: '#type-color-hex'
    - include: '#type-number'
    end: \)
    endCaptures:
      '0': {name: punctuation.section.function.css}

  type-function-rgba:
    begin: \b(rgba)(\()
    beginCaptures:
      '1': {name: support.function.rgba.css}
      '2': {name: punctuation.section.function.css}
    contentName: meta.function.rgba.css
    patterns:
    - include: '#comment-block'
    - include: '#type-function-calc'
    - include: '#type-percentage'
    - include: '#type-integer-non-negative'
    - name: constant.numeric.alphavalue.css
      match: 0(?:\.\d+)?|1(?:\.0+)?|\.\d+
    end: \)
    endCaptures:
      '0': {name: punctuation.section.function.css}

