  # The <content-list> value type in this rule is different from the
  # <content-list> type in #value-bookmark-label due to inconsistencies in the
  # spec. This will be resolved when CSS Generated Content Level 3 is rewritten.
  value-content:
    begin: (?<=content:)
    patterns:
    - include: '#comment-block'
    - include: '#type-important'
    - include: '#type-all'
    - include: '#type-function-attr'
    - include: '#type-function-toggle'
    - include: '#type-function-var'
    - name: support.constant.property-value.css
      match: \b(?:open-quote|normal|no-open-quote|no-close-quote|none|inhibit|contents|close-quote)\b
    - include: '#type-function-content'
    # The rule for counter() also covers counters()
    - include: '#type-function-counter'
    - include: '#type-function-leader'
    - include: '#type-function-running'
    - include: '#type-function-string'
    - include: '#type-function-target-counter'
    - include: '#type-function-target-counters'
    - include: '#type-function-target-text'
    - include: '#type-string-double'
    - include: '#type-string-single'
    - include: '#type-function-url'
    end: (?=[;)])

